{
    "name": "shasansetu",
    "version": "1.0.0",
    "private": true,
    "packageManager": "pnpm@9.0.0",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "clean": "turbo run clean",
        "db:generate": "pnpm --filter @shasansetu/db generate",
        "db:migrate": "pnpm --filter @shasansetu/db migrate",
        "db:push": "pnpm --filter @shasansetu/db push",
        "db:studio": "pnpm --filter @shasansetu/db studio"
    },
    "devDependencies": {
        "turbo": "^2.0.0",
        "typescript": "^5.3.0"
    }
}